<?xml version='1.0' encoding='utf-8'?>
<widget id="covid.satgas.biasawae" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SATGAS</name>
    <description>
        SATGAS COVID-19 KOTA BIASAWAE
    </description>
    <author email="hajirodeon@gmail.com" href="https://github.com/hajirodeon">
        SATGAS COVID-19 KOTA BIASAWAE
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://192.168.1.100/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="./res/icon/android/icon-36x36.png" />
        <icon density="mdpi" src="./res/icon/android/icon-48x48.png" />
        <icon density="hdpi" src="./res/icon/android/icon-72x72.png" />
        <icon density="xhdpi" src="./res/icon/android/icon-96x96.png" />
        <icon density="xxhdpi" src="./res/icon/android/icon-144x144.png" />
        <icon density="xxxhdpi" src="./res/icon/android/icon-196x196.png" />
        <splash density="land-ldpi" src="./res/screen/android/screen-320x200.png" />
        <splash density="land-mdpi" src="./res/screen/android/screen-480x320.png" />
        <splash density="land-hdpi" src="./res/screen/android/screen-800x480.png" />
        <splash density="land-xhdpi" src="./res/screen/android/screen-1280x720.png" />
        <splash density="port-ldpi" src="./res/screen/android/screen-200x320.png" />
        <splash density="port-mdpi" src="./res/screen/android/screen-320x480.png" />
        <splash density="port-hdpi" src="./res/screen/android/screen-480x800.png" />
        <splash density="port-xhdpi" src="./res/screen/android/screen-720x1280.png" />
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="30" />
    </platform>





<plugin name="cordova-plugin-camera" spec="2.4.1" />

  <plugin name="cordova-plugin-media-capture" source="npm" spec="1.4.1" >
    <variable name="CAMERA_USAGE_DESCRIPTION" value="App would like to access the camera." />

    <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="App would like to access the library." />
  </plugin>




    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyBZ73oHLqNFmGX6bs3qyyRAoCim-_WxdqQ" />
    <preference name="KeepRunning" value="true" />
    <plugin name="cordova-plugin-audioinput" spec="^1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />

    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-file" spec="~6.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="~3.1.0" />
    <plugin name="cordova-plugin-websocket" spec="~0.12.2" />
    <plugin name="cordova-plugin-gpslocation" spec="~1.0.0" />
    <plugin name="cordova-plugin-device-orientation" spec="~2.0.1" />
    <plugin name="cordova-plugin-dialogs" spec="~2.0.2" />
    <plugin name="cordova-plugin-geolocation" spec="~4.0.2" />
    <plugin name="cordova-plugin-local-notification" spec="~0.9.0-beta.2" />






<preference name="AndroidPersistentFileLocation" value="Compatibility" />
<preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />

<preference name="AndroidPersistentFileLocation" value="Internal" />


</widget>
